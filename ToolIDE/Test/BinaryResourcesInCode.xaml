<Window x:Class="ToolIDE.Test.BinaryResourcesInCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ToolIDE.Test"
        mc:Ignorable="d"
        Title="BinaryResourcesInCode" Height="450" Width="800">
        <Grid>
                <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                </Grid.RowDefinitions>
                <Button Content="Read Book Data" FontSize="14" Click="OnClick"/>
                <TextBox Grid.Row="1" IsReadOnly="True" x:Name="_text" FontSize="16" Margin="4"/>
        </Grid>
</Window>
